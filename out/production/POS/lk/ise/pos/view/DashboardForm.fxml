<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="dashboardContext" prefHeight="415.0" prefWidth="767.0"
            xmlns="http://javafx.com/javafx/19" fx:controller="lk.ise.pos.control.DashboardFormController">
    <children>
        <Label layoutX="24.0" layoutY="20.0" text="Dashboard">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <Separator layoutX="24.0" layoutY="51.0" prefHeight="6.0" prefWidth="733.0"/>
        <Label fx:id="txtDateAndTime" alignment="CENTER_RIGHT" contentDisplay="CENTER" layoutX="431.0" layoutY="25.0"
               prefHeight="17.0" prefWidth="251.0" text="Date and time"/>
        <Button layoutX="699.0" layoutY="21.0" mnemonicParsing="false" onAction="#logoutOnAction" text="Logout"/>
        <Button layoutX="14.0" layoutY="116.0" mnemonicParsing="false" onAction="#openCustomerForm" prefHeight="125.0"
                prefWidth="173.0" text="Customer"/>
        <Button layoutX="206.0" layoutY="116.0" mnemonicParsing="false" onAction="#openItemForm" prefHeight="125.0"
                prefWidth="173.0" text="Item"/>
        <Button layoutX="398.0" layoutY="116.0" mnemonicParsing="false" onAction="#openOrdersForm" prefHeight="125.0"
                prefWidth="173.0" text="Orders"/>
        <Button layoutX="587.0" layoutY="116.0" mnemonicParsing="false" onAction="#openNewOrderForm" prefHeight="125.0"
                prefWidth="173.0" text="New Order"/>
        <Button layoutX="303.0" layoutY="255.0" mnemonicParsing="false" onAction="#openIncomeForm" prefHeight="125.0"
                prefWidth="173.0" text="Income"/>
    </children>
</AnchorPane>
